  "private": true,
  "productName": "DeepNotes",
  "scripts": {
    "build:android": "cross-env NODE_OPTIONS=--max_old_space_size=4096 quasar build -m capacitor -T android --skip-pkg",
    "build:android:ide": "cross-env NODE_OPTIONS=--max_old_space_size=4096 quasar build -m capacitor -T android --skip-pkg --ide",
    "build:electron": "cross-env NODE_OPTIONS=--max_old_space_size=4096 quasar build -m electron",
    "build:electron:publish": "cross-env NODE_OPTIONS=--max_old_space_size=4096 quasar build -m electron --publish always",
    "build:ios": "cross-env NODE_OPTIONS=--max_old_space_size=4096 quasar build -m capacitor -T ios",
    "build:spa": "cross-env NODE_OPTIONS=--max_old_space_size=4096 quasar build -m spa",
    "build:ssr": "cross-env NODE_OPTIONS=--max_old_space_size=4096 quasar build -m ssr",
    "clean": "rimraf --glob ./dist *.tsbuildinfo",
    "dev:android": "quasar dev -m capacitor -T android",
    "dev:electron": "quasar dev -m electron",
    "dev:ios": "quasar dev -m capacitor -T ios",
    "dev:spa": "quasar dev -m spa",
    "dev:ssr": "quasar dev -m ssr",
    "fix": "eslint --fix --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "npkill": "rimraf --glob ./node_modules",
    "preinstall": "npx only-allow pnpm"
  }
