  "author": "Gustavo Toyota <gustavottoyota@gmail.com>",
  "dependencies": {
    "@deeplib/data": "workspace:*",
    "@deeplib/db": "workspace:*",
    "@deeplib/misc": "workspace:*",
    "@stdlib/base64": "workspace:*",
    "@stdlib/data": "workspace:*",
    "@stdlib/db": "workspace:*",
    "@stdlib/misc": "workspace:*",
    "@stdlib/redlock": "workspace:*",
    "cookie": "^0.5.0",
    "dotenv": "^16.3.1",
    "dotenv-expand": "9.0.0",
    "ioredis": "npm:@deepnotes/ioredis@5.3.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "2.3.0",
    "lib0": "^0.2.87",
    "lodash": "^4.17.21",
    "msgpackr": "^1.9.9",
    "objection": "3.0.1",
    "prom-client": "^15.0.0",
    "redlock": "5.0.0-beta.2",
    "unilogr": "^0.0.27",
    "ws": "8.11.0",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.8"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.3",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash": "^4.14.200",
    "@types/ws": "8.5.3",
    "tsup": "^7.2.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --build ./tsconfig.json && tsc-alias -p tsconfig.json",
    "build:watch": "concurrently \"tsc --build ./tsconfig.json --watch\" \"tsc-alias -p tsconfig.json --watch\"",
    "bundle": "tsup",
    "clean": "rimraf ./dist *.tsbuildinfo",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "fix": "eslint --fix --ext .js,.ts,.vue ./",
    "npkill": "rimraf ./node_modules",
    "preinstall": "npx only-allow pnpm",
    "repo:build": "tsc-alias -p tsconfig.json",
    "repo:build:watch": "tsc-alias -p tsconfig.json --watch",
    "start": "ts-node -r tsconfig-paths/register ./src/index.ts"
  }
}
